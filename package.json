{
  "name": "smallcase-icon-figma-plugin",
  "version": "0.0.1",
  "description": "smallcase icon figma plugin",
  "author": "Shubham Singh",
  "license": "MIT",
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch",
    "extract:icon": "ts-node ./glyph-extractor.ts"
  },
  "figma-plugin": {
    "id": "smallcase-icon",
    "name": "smallcase Icons",
    "main": "src/main.ts",
    "ui": "src/ui.tsx",
    "relaunchButtons": {
      "smallcaseIcons": {
        "name": "smallcase Icons",
        "main": "src/main.ts",
        "ui": "src/ui.tsx"
      }
    }
  },
  "dependencies": {
    "@create-figma-plugin/ui": "^1.3.3",
    "@create-figma-plugin/utilities": "^1.3.3",
    "@smallcase/shringar": "^6.6.0",
    "@xmldom/xmldom": "^0.7.2",
    "font-blast": "^0.7.0",
    "js-base64": "^3.6.1",
    "preact": "^10.5.14",
    "svgo": "^2.4.0",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^1.3.3",
    "@create-figma-plugin/tsconfig": "^1.3.3",
    "@figma/plugin-typings": "^1.31.0",
    "@types/node": "^16.7.1",
    "@types/svgo": "^2.4.0",
    "@types/xmldom": "^0.1.31",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "volta": {
    "node": "14.17.5"
  }
}
